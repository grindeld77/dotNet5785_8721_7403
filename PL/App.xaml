<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>
        <local:VolunteerCollection x:Key="VolunteerCollectionKey" />
        <local:OpenCallCollection x:Key="OpenCallCollectionKey" />
        <local:OpenCallCollectionType x:Key="OpenCallCollectionTypeKey" />
        <local:CallCollectionFilter x:Key="CallCollectionFilterKey" />
        <local:CallCollectionSort x:Key="CallCollectionSortKey" />
        <local:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />
        <local:SimulatorButtonContentConverter x:Key="SimulatorButtonContentConverter"/>

        <ResourceDictionary x:Key="Theme">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/LightTheme.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>